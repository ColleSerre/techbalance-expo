{
  "expo": {
    "name": "techbalance-expo",
    "slug": "techbalance-expo",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.darenpal.techbalanceexpo"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.darenpal.techbalanceexpo"
    },
    "web": {
      "bundler": "metro",
      "output": "server"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://techbalance-api.herokuapp.com"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": "https://techbalance-api.herokuapp.com"
      },
      "eas": {
        "projectId": "2cbb799a-7d85-4bdc-ad99-941cfab1ebbe"
      }
    },
    "owner": "darenpal"
  }
}